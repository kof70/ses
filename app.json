{
  "expo": {
    "name": "SES",
    "slug": "ses",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/adaptive-icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ses.app"
    },
    "android": {
      "package": "com.ses.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1e3a8a"
      },
      "permissions": [
        "CAMERA",
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "VIBRATE"
      ],
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      "expo-web-browser",
      "expo-asset",
      "expo-font"
    ],
    "scheme": "ses",
    "extra": {
      "eas": {
        "projectId": "7fde4581-2bce-4fc1-a1eb-7b9574d5eed7"
      }
    }
  }
}
