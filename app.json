{
  "expo": {
    "name": "SecureGuard Pro",
    "slug": "secureguard-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.secureguard.pro"
    },
    "android": {
      "package": "com.secureguard.pro",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1e3a8a"
      },
      "permissions": [
        "CAMERA",
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "VIBRATE"
      ],
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      "expo-web-browser",
      "expo-asset",
      "expo-font"
    ],
    "scheme": "secureguard",
    "extra": {
      "eas": {
        "projectId": "00290657-f980-4860-8962-15bffba296ae"
      }
    },
    "owner": "kof70"
  }
}
